FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         {{data.ddtSchemes.default}};
}

gradSchemes
{
    default         {{data.gradSchemes.default}};
}

divSchemes
{
    default         {{data.divSchemes.default}};
    div(phi,U)      Gauss linear;
    div((nuEff*dev2(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default         {{data.laplacianSchemes.default}};
}

interpolationSchemes
{
    default         {{data.interpolationSchemes.default}};
}

snGradSchemes
{
    default         {{data.snGradSchemes.default}};
}

