        {
            alpha     (500 -1000 -1000);
            beta      (0 0 0);
            rhoRef    1;
            coordinateSystem
            {
                origin    (0 0 0);
                e1        (0.70710678 0.70710678 0);
                e2        (0 0 1);
            }
        }
    }
