FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      delta;
}

dimensions  [0 0 0 0 0 0 0];

internalField  uniform 0.0;

boundaryField
{
    inlet
    {
        type    cyclic;
    }
    outlet
    {
        type    cyclic;
    }
    top
    {
        type    zeroGradient;
    }
    bottom
    {
        type    zeroGradient;
    }
    frontAndBackPlanes
    {
        type    empty;
    }
}
